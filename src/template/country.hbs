<div class="card">
    <div class="card-flag">
        <img src="{{flags.svg}}" alt="{{name.official}}">
    </div> 

    <h2 class="card-name">{{name.official}}</h2>

    <p class="card-text">Capital: {{capital}}</p>
    <p class="card-text">Population: {{population}}</p>
    <p class="card-text">Languages: {{#each languages}}{{this}}{{#unless @last}}, {{/unless}}{{/each}}</p>
 </div>
